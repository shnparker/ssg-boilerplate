{
  "name": "ssg-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "esw src/ -w --ext .js --ext .jsx",
    "lint:once": "eslint src/ --ext .js --ext .jsx",
    "lint:fix": "eslint src/ --ext .js --ext .jsx --fix",
    "lint:fix:preview": "eslint src/ --ext .js --ext .jsx --fix-dry-run",
    "packages:upgrade": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tailwindcss": "^1.4.6",
    "@tailwindcss/ui": "^0.3.0"
  },
  "devDependencies": {
    "proptypes": "1.1.0",
    "postcss-preset-env": "^6.7.0",
    "dotenv": "8.2.0",
    "eslint": "7.2.0",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-watch": "7.0.0"
  }
}
